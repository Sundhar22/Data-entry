{
  "name": "data-entry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:setup": "prisma migrate deploy && prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
